CREATE\040TABLE\040IF\040NOT\040EXISTS\040users\040(
\040\040id\040VARCHAR(255),
\040\040_id\040VARCHAR(255),
\040\040name\040VARCHAR(255),
\040\040email\040VARCHAR(255),
\040\040phone\040VARCHAR(50),
\040\040createdAt\040VARCHAR(50),
\040\040status\040VARCHAR(50),
\040\040qrCode\040TEXT
);
CREATE\040TABLE\040IF\040NOT\040EXISTS\040users\040(\040\040\040id\040VARCHAR(255),\040\040\040_id\040VARCHAR(255),\040\040\040name\040VARCHAR(255),\040\040\040email\040VARCHAR(255),\040\040\040phone\040VARCHAR(50),\040\040\040createdAt\040VARCHAR(50),\040\040\040status\040VARCHAR(50),\040\040\040qrCode\040TEXT\040);
